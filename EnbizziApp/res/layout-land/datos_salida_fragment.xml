<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <TextView
        android:id="@+id/lblFecha"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="25dp"
        android:paddingTop="25dp"
        android:text="@string/lblFecha" />

    <TextView
        android:id="@+id/txtFecha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/lblFecha"
        android:layout_toRightOf="@+id/lblFecha"
        android:layout_marginLeft="20dp"
        android:text="-" />

    <TextView
        android:id="@+id/lblRuta"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/lblFecha"
        android:paddingLeft="25dp"
        android:paddingTop="25dp"
        android:text="@string/lblRuta" />

    <TextView
        android:id="@+id/txtRuta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/lblRuta"
        android:layout_toRightOf="@+id/lblRuta"
        android:layout_alignLeft="@+id/txtFecha"
        android:text="-" />

    <TextView
        android:id="@+id/lblKm"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/lblRuta"
        android:paddingLeft="25dp"
        android:paddingTop="25dp"
        android:text="@string/lblKm" />

    <TextView
        android:id="@+id/txtKm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/lblKm"
        android:layout_toRightOf="@+id/lblKm"
        android:layout_alignLeft="@+id/txtFecha"
        android:text="-" />

    <TextView
        android:id="@+id/lblParada"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/lblKm"
        android:paddingLeft="25dp"
        android:paddingTop="25dp"
        android:text="@string/lblParada" />

    <TextView
        android:id="@+id/txtParada"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/lblParada"
        android:layout_toRightOf="@+id/lblParada"
        android:layout_alignLeft="@+id/txtFecha"
        android:text="-" />
    
    <!-- DERECHA -->

    <TextView
        android:id="@+id/lblRutaRetorno"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/txtFecha"
        android:layout_alignBottom="@+id/txtFecha"
        android:layout_marginLeft="75dp"
        android:text="@string/lblRutaRetorno" />

    <TextView
        android:id="@+id/txtRutaRetorno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/lblRutaRetorno"
        android:layout_toRightOf="@+id/lblRutaRetorno"
        android:layout_marginLeft="40dp"
        android:text="-" />

    <TextView
        android:id="@+id/lblDificultad"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/txtRuta"
        android:layout_alignBottom="@+id/txtRuta"
        android:layout_alignLeft="@+id/lblRutaRetorno"
        android:text="@string/lblDificultad" />

    <TextView
        android:id="@+id/txtDificultad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/lblDificultad"
        android:layout_toRightOf="@+id/lblDificultad"
        android:layout_alignLeft="@+id/txtRutaRetorno"
        android:text="-" />

    <TextView
        android:id="@+id/lblAltitudAcumulada"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/txtKm"
        android:layout_alignBottom="@+id/txtKm"
        android:layout_alignLeft="@+id/lblRutaRetorno"
        android:text="@string/lblAltitudAcumulada" />

    <TextView
        android:id="@+id/txtAltitudAcumulada"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/lblAltitudAcumulada"
        android:layout_toRightOf="@+id/lblAltitudAcumulada"
        android:layout_alignLeft="@+id/txtRutaRetorno"
        android:text="-" />
    
    
    <TextView
        android:id="@+id/lblTipo"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/txtParada"
        android:layout_alignBottom="@+id/txtParada"
        android:layout_alignLeft="@+id/lblRutaRetorno"
        android:text="@string/lblTipo" />

    <TextView
        android:id="@+id/txtTipo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/lblTipo"
        android:layout_toRightOf="@+id/lblTipo"
        android:layout_alignLeft="@+id/txtRutaRetorno"
        android:text="-" />

</RelativeLayout>